= render "shared/nav"
%div#users.container
  %div.panel.m_rounded_corners.box_shadow

    %div.m_header.m_top_rounded_corners
      %h1 All Users
    %div.content.m_bottom_rounded_corners
      %ul.no_style.horizontal
        - @users.each do |user|
          %li
            = link_to user.username, user_albums_path(user)
